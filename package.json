{
  "name": "minademian.com-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix-trailing": "eslint src e2e --ext .ts,.tsx,.js,.jsx --fix --rule 'no-trailing-spaces: error'",
    "debug-ci": "NODE_ENV=production pnpm tsc --project tsconfig.production.json --noEmit",
    "type-check": "pnpm run lint && pnpm run debug-ci",
    "pre-commit-check": "pnpm run lint && pnpm run debug-ci",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report"
  },
  "dependencies": {
    "framer-motion": "^11.0.3",
    "next": "14.1.1",
    "react": "^18",
    "react-dom": "^18",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8",
    "sass": "^1.70.0",
    "server-only": "^0.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
